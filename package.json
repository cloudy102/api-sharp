{
  "name": "api-sharp",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/api-sharp.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "npm run clean && tsdx build --format cjs,esm,umd --name ApiSharp",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "coverage": "jest --coverage",
    "clean": "[ -d 'dist' ] && rm -rf dist || echo 'no such directory \"dist\"'",
    "server": "json-server -p 4000 ./server/db.json --middlewares ./server/middleware.js",
    "debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.137",
    "@types/prop-types": "^15.7.2",
    "axios": "^0.19.0",
    "babel-jest": "^24.9.0",
    "husky": "^3.0.0",
    "json-server": "^0.15.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "semantic-release": "^15.13.24",
    "ts-node": "^8.3.0",
    "tsdx": "^0.7.2",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3",
    "xhr2": "^0.2.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "tiny-invariant": "^1.0.6",
    "tiny-warning": "^1.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whinc/api-sharp.git"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "testMatch": [
      "<rootDir>/test/*.test.ts"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/server/",
      "<rootDir>/node_modules/"
    ]
  }
}
