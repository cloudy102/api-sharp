{
  "name": "api-sharp",
  "version": "0.0.0-development",
  "main": "dist/index.js",
  "module": "dist/api-sharp.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "server": "json-server -p 4000 db.json",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "axios": "^0.19.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.137",
    "axios": "^0.19.0",
    "babel-jest": "^24.9.0",
    "husky": "^3.0.0",
    "json-server": "^0.15.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "semantic-release": "^15.13.24",
    "ts-node": "^8.3.0",
    "tsdx": "^0.7.2",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3",
    "xhr2": "^0.2.0"
  },
  "dependencies": {
    "tiny-invariant": "^1.0.6"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "testMatch": [
      "<rootDir>/test/*.ts"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/\\w+\\.json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whinc/api-sharp.git"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
